{
  "manifest_version": 3,
  "name": "Options Calculator Extension",
  "version": "1.0.0",
  "description": "A simple options calculator extension.",
  "action": {
    "default_popup": "public/popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["activeTab"]
}
